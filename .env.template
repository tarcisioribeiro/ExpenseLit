# ExpenseLit Streamlit App - Configurações de Ambiente
# Copie este arquivo para .env e configure com seus valores

# Configurações da API ExpenseLit
API_BASE_URL=http://localhost:8002

# Configurações do PostgreSQL Principal (ExpenseLit API)
DB_HOSTNAME=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=sua_senha_postgres_principal_segura
DB_NAME=expenselit

# Configurações de Debug (opcional)
DEBUG=true

# Configurações de autenticação (opcional - para testes)
DEFAULT_USERNAME=admin
DEFAULT_PASSWORD=sua_senha_admin_segura